{
  "name": "LekkaLink",
  "version": "1.0",
  "license": "MIT",
  "summary": "LekkaLink is a Cocoa framework to automatically capture screenshots",
  "homepage": "https://gitlab.com/lekka/lekka-link-ios",
  "authors": "Peeka Sharma",
  "source": {
    "git": "https://gitlab.com/lekka/lekka-link-ios.git",
    "tag": "1.0"
  },
  "requires_arc": true,
  "dependencies": {
    "Alamofire": [
      "~> 5.0.5"
    ],
    "SVProgressHUD": [

    ],
    "ReachabilitySwift": [

    ],
    "DeviceKit": [
      "~> 2.0"
    ]
  },
  "static_framework": true,
  "platforms": {
    "ios": "10.0"
  },
  "swift_versions": "5",
  "xcconfig": {
    "LIBRARY_SEARCH_PATHS": "\"$(PODS_ROOT)/Library/libLekkaLink\"",
    "HEADER_SEARCH_PATHS": "\"${PODS_ROOT}/Headers/Library/libLekkaLink\""
  },
  "preserve_paths": "Library/*",
  "swift_version": "5"
}
