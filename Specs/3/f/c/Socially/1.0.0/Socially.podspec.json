{
  "name": "Socially",
  "version": "1.0.0",
  "summary": "Social Sign-In for SwiftUI",
  "description": "Socially provides the easiest way to integrate Social Sign-In in iOS app using SwiftUI",
  "homepage": "https://github.com/phoelapyae69/Socially",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "phoelapyae69": "phoelapyayt7@gmail.com"
  },
  "source": {
    "git": "https://github.com/phoelapyae69/Socially.git",
    "tag": "1.0.0"
  },
  "social_media_url": "https://twitter.com/phoelapyaeX_X",
  "platforms": {
    "ios": "13.0"
  },
  "source_files": "Socially/Classes/**/*",
  "swift_versions": "5.2",
  "static_framework": true,
  "subspecs": [
    {
      "name": "Google",
      "source_files": [
        "Socially/Classes/Google/*",
        "Socially/Classes/SociallyAuth/*"
      ],
      "dependencies": {
        "GoogleSignIn": [
          "~> 5.0"
        ]
      }
    },
    {
      "name": "Facebook",
      "source_files": [
        "Socially/Classes/Facebook/*",
        "Socially/Classes/SociallyAuth/*"
      ],
      "dependencies": {
        "FBSDKLoginKit": [
          "~> 7.0"
        ]
      }
    }
  ],
  "swift_version": "5.2"
}
